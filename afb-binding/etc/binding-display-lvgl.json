{
    "name": "afb-display",
    "port": 1234,
    "alias": [
        "/devtools:/usr/share/afb-ui-devtools/binder"
    ],
    "monitoring": true,
    "binding": [
        {
            "uid": "display-lvgl",
            "api": "display",
            "path": "${CARGO_TARGET_DIR}debug/libafb_lvgl.so",
            "info": "lvgl embedded display binding",
            "permission": "acl:display:client",
            "display": {
                "x_res": 1024,
                "y_res": 800,
                "ration": 1
            }
        }
    ]
}